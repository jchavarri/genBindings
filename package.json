{
  "name": "genbindings",
  "version": "1.0.0",
  "description": "Babel plugin to auto-generate Reason bindings from JavaScript+Flow annotated code.",
  "repository": "jchavarri/genBindings",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "bsb -make-world",
