    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.3.13",
    "bs-platform": "^4.0.5",
    "chalk": "^1.1.0",
    "clear": "0.0.1",
    "diff": "^3.2.0",
    "flow-bin": "^0.83.0",
    "watch": "^1.0.1"
